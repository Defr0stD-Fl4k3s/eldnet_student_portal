@{
}
@model IEnumerable<StudentPortal.Web.Models.Entities.EnrollmentDetailFile>

<table class="table">
    <thead>
        <tr>
            <th>Student ID</th>
            <th>EDP Code</th>
            <th>Subject Code</th>
            <th>Status</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ENRDFSTUDID</td>
                <td>@item.ENRDFSTUDEDPCODE</td>
                <td>@item.ENRDFSTUDSUBJCDE</td>
                <td>@item.ENRDFSTUDSTATUS</td>
                <td>
                    <!-- Edit Link -->
                    <a href="@Url.Action("Edit", "EnrollmentDetail", new { studentId = item.ENRDFSTUDID, edpCode = item.ENRDFSTUDEDPCODE })">Edit</a> |
                   
                </td>
            </tr>
        }
    </tbody>
</table>
