@{
}
@model IEnumerable<StudentPortal.Web.Models.Entities.EnrollmentHeaderFile>

@{
    ViewData["Title"] = "Enrollment Header List";
}

<h2>@ViewData["Title"]</h2>

<table class="table">
    <thead>
        <tr>
            <th>Student ID</th>
            <th>Enrollment Date</th>
            <th>School Year</th>
            <th>Encoder</th>
            <th>Total Units</th>
            <th>Status</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ENRHFSTUDID</td>
                <td>@item.ENRHFSTUDDATEENROLL.ToString("yyyy-MM-dd")</td>
                <td>@item.ENRHFSTUDSCHLYR</td>
                <td>@item.ENRHFSTUDENCODER</td>
                <td>@item.ENRHFSTUDTOTALUNITS</td>
                <td>@item.ENRHFSTUDSTATUS</td>
                <td>
                    <a href="@Url.Action("Edit", new { id = item.ENRHFSTUDID })">Edit</a> |
                    <form action="@Url.Action("Delete", new { id = item.ENRHFSTUDID })" method="post" style="display:inline" onsubmit="return confirm('Are you sure you want to delete this item?');">
                        <button type="submit">Delete</button>
                    </form>
                </td>
            </tr>
        }
    </tbody>
</table>
